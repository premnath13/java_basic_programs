import java.util.*;
public class main {
    public static void main(Strings[] args){
      Scanner sc = new Scanner(System.in);
      int num,i;
      System.out.print("Enter The Number For Multplication Table:");
      num = sc.nextInt();
      for (i=0;i<=16;i++){
      System.out.println("%d * %d = %d",num,i,num*i);
      }
   }
}
      
